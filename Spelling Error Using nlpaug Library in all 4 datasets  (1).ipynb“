harts = await google.colab.kernel.invokeFunction(\n","            'suggestCharts', [key], {});\n","      } catch (error) {\n","        console.error('Error during call to suggestCharts:', error);\n","      }\n","      quickchartButtonEl.classList.remove('colab-df-spinner');\n","      quickchartButtonEl.classList.add('colab-df-quickchart-complete');\n","    }\n","    (() => {\n","      let quickchartButtonEl =\n","        document.querySelector('#df-f27c4619-957c-45fb-8482-02f0d25e7594 button');\n","      quickchartButtonEl.style.display =\n","        google.colab.kernel.accessAllowed ? 'block' : 'none';\n","    })();\n","  </script>\n","</div>\n","    </div>\n","  </div>\n"]},"metadata":{},"execution_count":15}]},{"cell_type":"markdown","source":["**Code to introduce spelling error with error probablity =1**"],"metadata":{"id":"pU59dQgmsUpg"}},{"cell_type":"code","source":["aug = nac.KeyboardAug()\n","augmented_texts = []\n","error_probability = 1  # Define the probability of introducing spelling errors\n","\n","# Iterate through each row in the 'text' column and apply augmentation\n","for index, row in df_final.iterrows():\n","    original_text = row['cleaned_text']\n","\n","    # Check if spelling error should be induced or not based on the error_probability\n","    if random.random() <= error_probability:\n","        aug = nac.RandomCharAug(action=\"swap\")\n","        augmented_text = aug.augment(original_text)\n","        if augmented_text:\n","            # Append the first item of augmented_text to the list\n","            augmented_texts.append(augmented_text[0])\n","        else:\n","            # If augmented_text is empty, append an empty string or handle it accordingly\n","            augmented_texts.append(\"\")\n","    else:\n","        # If no error is induced, use the original text\n","        augmented_texts.append(original_text)\n","\n","# Create a new column 'augmented_text' and assign the augmented texts to it\n","df_final['augmented_text1'] = augmented_texts"],"metadata":{"id":"QHtTx4V6W768"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["**Code to introduce spelling error with error probablity =0.5**"],"metadata":{"id":"ciFi4d3ysx_J"}},{"cell_type":"code","source":["aug = nac.KeyboardAug()\n","augmented_texts = []\n","error_probability = 0.5  # Define the probability of introducing spelling errors\n","\n","# Iterate through each row in the 'text' column and apply augmentation\n","for index, row in df_final.iterrows():\n","    original_text = row['cleaned_text']\n","\n","    # Check if spelling error should be induced or not based on the error_probability\n","    if random.random() <= error_probability:\n","        aug = nac.RandomCharAug(action=\"swap\")\n","        augmented_text = aug.augment(original_text)\n","        if augmented_text:\n","            # Append the first item of augmented_text to the list\n","            augmented_texts.append(augmented_text[0])\n","        else:\n","            # If augmented_text is empty, append an empty string or handle it accordingly\n","            augmented_texts.append(\"\")\n","    else:\n","        # If no error is induced, use the original text\n","        augmented_texts.append(original_text)\n","\n","# Create a new column 'augmented_text' and assign the augmented texts to it\n","df_final['augmented_text0.5'] = augmented_texts"],"metadata":{"id":"KpFVEccZYNIL"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["**Code to calculate the percentage of word difference between cleaned_text and augmented_text1**"],"metadata":{"id":"WJLOR6hltRAj"}},{"cell_type":"code","source":["# Function to calculate the percentage of word difference\n","def calculate_word_difference_percentage(cleaned_text, augmented_text):\n","\n","  # Tokenize the texts into words (you can use a more sophisticated tokenizer if needed)\n","  cleaned_words = cleaned_text.split()\n","  augmented_words = augmented_text.split()\n","\n","  cleaned_text_len = len(cleaned_words)\n","  augmented_words_len = len(augmented_words)\n","\n","  # Convert the word lists to sets for efficient comparison\n","  augmented_words_set1 = set(cleaned_words)\n","  augmented_words_set2 = set(augmented_words)\n","\n","  # Find the words that are unique to each sentence\n","  unique_to_sentence1 = augmented_words_set1.difference(augmented_words_set2)\n","  different_word_count = len(unique_to_sentence1)\n","  percentage = different_word_count/cleaned_text_len * 100\n","  return percentage\n","\n","plist = []\n","for index, row in df_final.iterrows():\n","    cleaned_txt = row['cleaned_text']\n","    aug_prob1_text = row['augmented_text1']\n","    percentage_difference1 = calculate_word_difference_percentage(cleaned_txt, aug_prob1_text)\n","    plist.append(percentage_difference1)\n","\n","#print(f\"Percentage of word difference:\", plist)\n","df_final['Error_percentage1'] =plist\n","df_final"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"4nXIi6Sl2gAL","executionInfo":{"status":"ok","timestamp":1700921465345,"user_tz":-60,"elapsed":396,"user":{"displayName":"Warsi Sana Ali","userId":"07075384892740316485"}},"outputId":"15ff0c67-fe59-4c9b-90e5-3bf0f76b2e58"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["                                                  text sentiment  \\\n","0    @nordschaf theoretisch kannste dir überall im ...  positive   \n","1    Bahn verspätet sich..gleich kommt noch jemand ...  negative   \n","2    Kann ich mit dem DB Geschenk Ticket den ICE Sp...   neutral   \n","3    Re: Frank Hennemann \"Tolle Veranstaltung der C...   neutral   \n","4    RT @KleineHyaene: \"Mama, der bei der Bahn war ...  negative   \n","..                                                 ...       ...   \n","183  Diese gar nicht nationalistische Band spielt n...   neutral   \n","184  #Job #Stellenangebot http://t.co/Sd6jUJnCsg - ...   neutral   \n","185  Deutsche Bahn i