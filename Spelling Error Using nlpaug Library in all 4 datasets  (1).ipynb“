0.5'] =plist\n","df_final"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"_7iFnYqzPJeB","executionInfo":{"status":"ok","timestamp":1700921481512,"user_tz":-60,"elapsed":722,"user":{"displayName":"Warsi Sana Ali","userId":"07075384892740316485"}},"outputId":"307c5ad4-138c-4a59-d46b-753d64cc109c"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["                                                  text sentiment  \\\n","0    @nordschaf theoretisch kannste dir überall im ...  positive   \n","1    Bahn verspätet sich..gleich kommt noch jemand ...  negative   \n","2    Kann ich mit dem DB Geschenk Ticket den ICE Sp...   neutral   \n","3    Re: Frank Hennemann \"Tolle Veranstaltung der C...   neutral   \n","4    RT @KleineHyaene: \"Mama, der bei der Bahn war ...  negative   \n","..                                                 ...       ...   \n","183  Diese gar nicht nationalistische Band spielt n...   neutral   \n","184  #Job #Stellenangebot http://t.co/Sd6jUJnCsg - ...   neutral   \n","185  Deutsche Bahn investiert weiter massiv in die ...   neutral   \n","186  Die Bahn wird, laut Aussage Grubes, in Zukunft...   neutral   \n","187  Re: Detlef Steves Ja schon,aber das haben sie ...   neutral   \n","\n","                                          cleaned_text  \\\n","0    nordschaf theoretisch kannste überall Kölner S...   \n","1    Bahn verspätet gleich kommt jemand drückt ries...   \n","2    Kann DB Geschenk Ticket ICE Sprinter nutzen Si...   \n","3    Re Frank Hennemann Tolle Veranstaltung CDU heu...   \n","4    KleineHyaene Mama Bahn inkompetent beim Rausge...   \n","..                                                 ...   \n","183  Diese gar nationalistische Band spielt nachher...   \n","184  Job Stellenangebot Deutsche Bahn AG Quereinsti...   \n","185  Deutsche Bahn investiert massiv Infrastruktur ...   \n","186  Die Bahn laut Aussage Grubes Zukunft Flotten f...   \n","187  Re Detlef Steves Ja s